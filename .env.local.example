# For local development, copy this file to .env.local and replace the values with your Supabase credentials.
# Do not commit .env.local to your git repository.

NEXT_PUBLIC_SUPABASE_URL=https://rxcpcrvvatwwoeeehvqp.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Variables for the lock/unlock mechanism.
# The PI_SERVO_URL is the endpoint for your Raspberry Pi server that controls the servo.
# This URL is likely from a service like ngrok and may be temporary.
PI_SERVO_URL=https://your-ngrok-url.ngrok-free.app/
PI_SERVO_SECRET=your_secret_for_signing_requests

# Mailgun credentials for email notifications on password attempts
MAILGUN_API_KEY=your_mailgun_api_key
MAILGUN_DOMAIN=your_mailgun_domain
